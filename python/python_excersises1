for i in range(1,20):
    if(i%3==0):
        print(i)

result = 0
index = 0
while index <= 50:
    if index % 2 == 0:
        result+=index
    index+=1
print(result)

lst = [5,8,2,15,10,3,7]
for i, val in enumerate(lst):
    if val >5:
        print(val)

lst = [1,2,3,4,5]
lst2=[1,3,6,10,15]
lst2.append(lst[0])

def swap(list):
    temp = lst[0]
    lst[0] = lst[-1]
    lst[-1] = temp
    return lst

lst = [1,2,3,4,5]
swap(lst)
print(lst)
